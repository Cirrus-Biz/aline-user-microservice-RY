pipeline {
    agent {
        docker {
            image 'maven:3.8.1-adoptopenjdk-11' 
            args '-v /root/.m2:/root/.m2' 
        }
    }
    stages {
        stage('Package') { 
            steps {
                sh 'mvn clean package -DskipTests' 
            }
        }
        stage('Build') {
            steps {
                app = docker.build("./aline-user-microservice-RY")
            }
        }
    }
}