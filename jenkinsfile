pipeline {
    environment {
        registry = "noodleween/aline-user-ry"
        registryCredential = 'dockerhub'
        dockerImage = ''
    }
    agent {
        any
    }
    tools {
        maven
        docker
    }
    stages {
        stage('Package') { 
            steps {
                sh 'mvn clean package -DskipTests' 
            }
        }
        stage('Build') {
            steps {
                sh "docker build -t noodleween/aline-user-ry"
            }
        }
    }
}