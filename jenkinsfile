pipeline {
    agent {
        dockerfile true
        docker {
            image 'maven:3.8.1-adoptopenjdk-11' 
            args '-v /root/.m2:/root/.m2' 
        }
    }
    stages {
        stage('Package') { 
            steps {
                sh 'mvn clean package -DskipTests' 
            }
        }
        stage('Build') {
            steps {
                sh 'docker build -t noodleween/aline-user-ry:corretto .'
            }
        }
    }
}